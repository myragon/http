{
    "name": "myragon/http",
    "description": "Http package for PHP",
    "keywords": [
        "http"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/myragon",
    "scripts": {
        "csfix": "./vendor/bin/php-cs-fixer fix",
        "phpstan": "./vendor/bin/phpstan analyse -c phpstan.neon"
    },
    "autoload": {
        "psr-4": {
            "Myragon\\Http\\": "src/"
        }
    },
    "authors": [
        {
            "name": "bdumitrua",
            "email": "bdumitrua@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.4",
        "myragon/support": "dev-main"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.84",
        "phpstan/phpstan": "^2.1"
    }
}